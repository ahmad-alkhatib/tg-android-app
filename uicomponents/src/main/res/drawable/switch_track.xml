<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/switch_radius" />
            <solid android:color="@color/switch_disable_bg" />
            <stroke android:color="@color/switch_disable_bg" android:width="@dimen/switch_stroke"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/switch_radius" />
            <solid android:color="@color/switch_enable_bg" />
            <stroke android:color="@color/switch_enable_bg" android:width="@dimen/switch_stroke"/>
        </shape>
    </item>
</selector>